{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run -s clean && npm run -s lint",
    "build:static": "mkdir dist && cp -r ./public/* dist",
    "build:js": "webpack -p",
    "build": "npm run -s build:static && npm run -s build:js && npm run -s cache",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint .",
    "lint": "npm run -s lint:js && npm run -s lint:css",
    "start": "webpack-dev-server --progress --inline --hot",
    "watch:test": "npm run -s test -- --watch",
    "cache": "sw-precache --root dist --config sw-precache-config.json --cacheId $npm_package_name",
    "test": "ava",
    "coverage": "nyc npm -s test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.11.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "sw-precache": "^4.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "nyc": {
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "require": "babel-register",
    "reporter": [
      "html",
      "text"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "all": true
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "require": "babel-register",
    "babel": "inherit"
  }
}
